ahpFile <- system.file("extdata", "apu2.ahp", package="ahp")
cat(readChar(ahpFile, file.info(ahpFile)$size))
daneAhp <- Load("apu2.ahp")
Calculate(daneAhp)
Analyze(daneAhp)
AnalyzeTable(daneAhp)
PrioritiesFromPairwiseMatrixMeanNormalization(daneAhp)
